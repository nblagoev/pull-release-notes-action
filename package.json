{
  "name": "pull-release-notes-action",
  "version": "1.0.2",
  "private": true,
  "description": "A GitHub Action to generate a PR changelog between two refs.",
  "author": "Nikolay Blagoev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:nblagoev/pull-release-notes-action.git"
  },
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm test"
  },
  "keywords": [
    "actions",
    "changelog",
    "release-notes",
    "pull-request"
  ],
  "dependencies": {
    "@actions/core": "^1.9.1",
    "@nblagoev/pull-release-notes": "^1.2.6"
  },
  "devDependencies": {
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  }
}
